{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "block-scoped-var": [
            "off"
        ],
        "no-unused-vars": [
            "off"
        ],
        "no-console": [
            "off"
        ],
        "no-undef": [
            "off"
        ],
        "no-redeclare":[
            "off"
        ]
    }
}